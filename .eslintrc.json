{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    /*"extends": [
     //   "standard"
     "eslint" :""
    ],*/
    "globals": {
        //   "Atomics": "readonly",
        //   "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "semi": "warn",
        "quotes": [
            "warn",
            "single"
        ],
        "no-unused-vars": "warn",
        "no-case-declarations" :"off",
        "jsx-quotes":"warn"
        //    "space-before-blocks":"warn"
    }
}